## Model Architecture ##
########################

"model_type": "mlp", # type: str, currently mlp only option (mlp with 0 hidden layers is linear)

"n_hid_layers": [1], # type: int

"n_hid_units": [32], # type: int

"activation": "relu" # type: str, could be linear, relu, lrelu, sigmoid, tanh

}
